        "@types/jquery": "^3.5.14",
        "@types/node": "^16.11.27",
        "@types/scrollreveal": "^0.0.8",
        "@vitejs/plugin-vue": "^2.3.1",
        "@vitejs/plugin-vue-jsx": "^1.3.10",
        "@vue/eslint-config-typescript": "^10.0.0",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.5.0",
        "eslint-plugin-vue": "^8.2.0",
        "sass": "^1.51.0",
        "typescript": "~4.6.3",
        "vite": "^2.9.5",
